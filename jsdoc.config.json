{
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-react"
  ],
  "recurseDepth": 10,
  "source": {
    "include":  ["."],
    "includePattern": ".+\\.([jt]s(doc|x))?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [ "jsdoc", "closure" ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}